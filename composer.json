{
    "name": "anam/phpcart",
    "description": "Simple framework agnostic shopping cart",
    "keywords": [
        "Cart",
        "php cart",
        "Shopping cart",
        "Laravel shopping cart",
        "Laravel cart"
    ],
    "homepage": "https://github.com/anam-hossain/phpcart",
    "license": "MIT",
    "authors": [
        {
            "name": "Anam hossain",
            "email": "enam33@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/http-foundation": "~3.0|~4.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Anam\\Phpcart\\" : "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anam\\Phpcart\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anam\\Phpcart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Anam\\Phpcart\\Facades\\Cart"
            }
        }
    }
}
